# AG-X 2026 Environment Configuration
# =====================================
# Copy this file to .env and customize

# Environment
AGX_ENV=development
AGX_DEBUG=false
AGX_LOG_LEVEL=INFO

# Paths
AGX_DATA_DIR=./data
AGX_EXPERIMENTS_DIR=./experiments
AGX_REPORTS_DIR=./reports

# Compute
AGX_USE_GPU=false
AGX_NUM_WORKERS=4
AGX_RANDOM_SEED=42

# Web Dashboard
AGX_WEB_HOST=0.0.0.0
AGX_WEB_PORT=8050
